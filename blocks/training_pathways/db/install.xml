<?xml version="1.0" encoding="UTF-8" ?>
  <XMLDB PATH="blocks/training_pathways/db" VERSION="20170404" COMMENT="XMLDB file for Moodle blocks/training_pathways">
    <TABLES>
      <TABLE NAME="block_training_pathways" COMMENT="Stores information about Training Pathway courses">
        <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="regions" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="roles" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="informationurl" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="experienced" TYPE="int" LENGTH="2" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      </TABLE>
    </TABLES>
  </XMLDB>
  